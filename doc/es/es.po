"<guimenu><accel>F</accel>ile</guimenu> <guimenuitem>Export S<accel>V</"
"accel>G ...</guimenuitem>"
msgstr ""
"<guimenu><accel>A</accel>rchivo</guimenu> <guimenuitem>Exportar S<accel>V</"
"accel>G ...</guimenuitem>"

